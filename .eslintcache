\nimport { Link } from 'react-router-dom'\nimport './css/Header.css'\nimport { auth } from './Firebase';\nimport { useStateValue } from './StateProvider';\n\nfunction Header() {\n    const [state, dispatch] = useStateValue();\n\n    const signInHandler = () => {\n        if (state.user) {\n            auth.signOut();\n        }\n    }\n\n    return (\n        <div className='header'>\n            <Link to='/'>\n            <img\n                className='header__logo'\n                src='http://pngimg.com/uploads/amazon/amazon_PNG25.png'\n                alt='Amaclone' />\n            \n            </Link>\n\n            <div className='header__search'>\n                <input\n                className='header__searchInput' \n                    type='text'\n                    placeholder='Amaclone!!!'\n                />\n                <i className='fas fa-search header__searchIcon'></i>\n\n            </div>\n\n            <div className='header__nav'>\n                <Link to={!state.user && '/login'}>\n                <div onClick={signInHandler} className='header__option'>\n                    <span className='header__optionLineOne'>{state.user ? `Hello ${state.user.email}` : 'Hello Guest'}</span>\n                    <span className='header__optionLineTwo'>{state.user ? 'Sign Out' : 'Sign In'}</span>\n                </div>\n                </Link>\n\n                <div className='header__option'>\n                    <span className='header__optionLineOne'>Returns</span>\n                    <span className='header__optionLineTwo'>& Orders</span>\n                </div>\n\n                <div className='header__option'>\n                    <span className='header__optionLineOne'>Your</span>\n                    <span className='header__optionLineTwo'>Prime</span>\n                </div>\n\n                <Link to='/checkout' >\n                    <div className='header__optionBag'>\n                        <i className=\"fas fa-shopping-bag\"></i>\n                        <span className='header__optionLineTwo header__bagCount'>\n                            {state.bag?.length}\n                        </span>\n                        \n                    </div>\n                </Link>\n\n            </div>\n        </div>\n    )\n}\n\nexport default Header\n",["69","70"],"/Users/Cannigia/Code/React/amaclone/src/Home.js",[],"/Users/Cannigia/Code/React/amaclone/src/Product.js",["71"],"/Users/Cannigia/Code/React/amaclone/src/Checkout.js",["72"],"/Users/Cannigia/Code/React/amaclone/src/Subtotal.js",["73"],"import React from 'react';\nimport './css/Subtotal.css';\nimport CurrencyFormat from 'react-currency-format';\nimport { useStateValue } from './StateProvider';\nimport { getBagTotal } from './Reducer';\nimport { useHistory } from 'react-router-dom';\n\nfunction Subtotal() {\n\n    const history = useHistory();\n    const [state, dispatch] = useStateValue();\n\n    return (\n        <div className='subtotal'>\n            <CurrencyFormat\n                renderText={(value) => (\n                    <>\n                        <p>\n                            {/* Part of the homework */}\n                            Subtotal ({state.bag.length} items):\n                                <strong> {value}</strong>\n                        </p>\n                        <small className='subtotal__gift'>\n                            <input type='checkbox' />\n                            This order contains a gift\n                        </small>\n                    </>\n                )}\n                decimalScale={2} //allows 2 digits of decimal number\n                value={getBagTotal(state.bag)} // Part of homework\n                displayType={'text'}\n                thousandSeparator={true}\n                prefix={'$'}\n            />\n\n            <button onClick={e => history.push('/payment')}> \n                Proceed to Checkout\n                </button>\n        </div>\n        \n    )\n}\n\nexport default Subtotal\n","/Users/Cannigia/Code/React/amaclone/src/Reducer.js",[],"/Users/Cannigia/Code/React/amaclone/src/StateProvider.js",[],"/Users/Cannigia/Code/React/amaclone/src/CheckoutProduct.js",["74"],"import React from 'react'\nimport './css/CheckoutProduct.css'\nimport { useStateValue } from './StateProvider'\n\n\nfunction CheckoutProduct(props) {\n    const [state, dispatch] = useStateValue();\n\n    //removes item from basket\n    const removeFromBasket = () => {\n        dispatch({\n            type: 'REMOVE_FROM_BAG',\n            id: props.id,\n        })\n    }\n\n    return (\n        <div className='checkoutProduct'>\n            <img className='checkoutProduct__image' src={props.image} alt='' />\n\n            <div className='checkoutProduct__info'>\n                <p className='checkoutProduct__title'>{props.title}\n                </p>\n                <p className='checkoutProduct__price'>\n                    <small>$</small>\n                    <strong>{props.price}</strong>\n                </p>\n                <div className='checkoutProduct__rating'>\n                {Array(props.rating)\n                    .fill()\n                    .map((_, i) => (\n                        <p>⭐️</p>\n                    ))\n                }\n                </div>\n                <button onClick={removeFromBasket}>Remove from Bag</button>\n            </div>\n        </div>\n    )\n}\n\nexport default CheckoutProduct\n","/Users/Cannigia/Code/React/amaclone/src/Login.js",[],"/Users/Cannigia/Code/React/amaclone/src/Firebase.js",[],"/Users/Cannigia/Code/React/amaclone/src/Payment.js",["75"],"/Users/Cannigia/Code/React/amaclone/src/Axios.js",[],"/Users/Cannigia/Code/React/amaclone/src/Orders.js",[],{"ruleId":"76","severity":1,"message":"77","line":22,"column":10,"nodeType":"78","messageId":"79","endLine":22,"endColumn":12},{"ruleId":"80","severity":1,"message":"81","line":44,"column":6,"nodeType":"82","endLine":44,"endColumn":8,"suggestions":"8